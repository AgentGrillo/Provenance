{
  "name": "Atari800",
  "version": "1.5",
  "summary": "Atari800 Provenance Emulator Cores",
  "description": "Atari800 is the iOS framework for the Provenance Atari800 emulator core.\nUpsream core http://sourceforge.net/projects/atari800/",
  "homepage": "https://provenance-emu.com",
  "license": {
    "type": "Provenance License",
    "file": "LICENSE.md"
  },
  "authors": {
    "James Addyman": "james@provenance-emu.com",
    "Joseph Mattiello": "joe@provenance-emu.com"
  },
  "source": {
    "git": "https://github.com/Provenance/Provenance.git",
    "tag": "1.5"
  },
  "cocoapods_version": ">= 1.8.0",
  "swift_versions": [
    "5.0",
    "5.1"
  ],
  "platforms": {
    "ios": "10.0",
    "tvos": "10.0"
  },
  "module_name": "Atari800",
  "header_dir": "Atari800",
  "frameworks": "Foundation",
  "libraries": [
    "z",
    "edit"
  ],
  "dependencies": {
    "PVSupport": [
      "~> 1.5"
    ],
    "PVLibrary": [
      "~> 1.5"
    ]
  },
  "pod_target_xcconfig": {
